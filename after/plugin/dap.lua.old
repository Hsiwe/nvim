local dap = require("dap")

vim.keymap.set("n", "<F2>", function()
	dap.toggle_breakpoint()
end)

vim.keymap.set("n", "<F3>", function()
	dap.repl.open()
end)

vim.keymap.set("n", "<F5>", function()
	dap.continue()
end)

vim.keymap.set("n", "<F10>", function()
	dap.step_over()
end)

vim.keymap.set("n", "<F11>", function()
	dap.step_into()
end)

